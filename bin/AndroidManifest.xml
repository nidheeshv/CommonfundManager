    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity
            android:name="com.commonfundmanager.activities.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
       
        
        <activity android:name="com.commonfundmanager.activities.MemberHomeActivity" />
        <activity android:name="com.commonfundmanager.activities.ClaimActivity" />
        <activity android:name="com.commonfundmanager.activities.AccountInfoActivity" />
        <activity android:name="com.commonfundmanager.activities.MyRequestsActivity" />
        <activity android:name="com.commonfundmanager.activities.AllDeposits" />
        <activity android:name="com.commonfundmanager.activities.AllRedemptions" />
        <activity android:name="com.commonfundmanager.activities.UpdateClaimActivity" />
        
        
    </application>
</manifest>
